{
  "name": "kouchou-ai",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "lint": "biome check .",
    "format": "biome check --write .",
    "test:e2e": "pnpm --filter @kouchou-ai/e2e-tests test",
    "test:e2e:ui": "pnpm --filter @kouchou-ai/e2e-tests test:ui",
    "test:e2e:report": "pnpm --filter @kouchou-ai/e2e-tests report",
    "build:packages": "pnpm --filter './packages/*' build",
    "clean": "pnpm -r exec rm -rf node_modules dist .next"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@playwright/test": "^1.56.0",
    "lefthook": "^1.11.8"
  },
  "dependencies": {
    "@next/third-parties": "^15.2.4"
  },
  "pnpm": {
    "overrides": {
      "react": "19.2.3",
      "react-dom": "19.2.3"
    }
  }
}
